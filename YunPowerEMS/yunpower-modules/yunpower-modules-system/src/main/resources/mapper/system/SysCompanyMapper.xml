        <if test="stopFlag != null "> and stop_flag = #{stopFlag}</if>
        <if test="deleteFlag != null "> and delete_flag = #{deleteFlag}</if>
    </select>

    <select id="selectSysCompanyById" parameterType="Long" resultMap="SysCompanyResult">
