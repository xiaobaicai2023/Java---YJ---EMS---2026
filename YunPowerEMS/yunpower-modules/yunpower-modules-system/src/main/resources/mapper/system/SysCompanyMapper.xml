        <if test="address != null  and address != ''"> and address = #{address}</if>
        <if test="postCode != null  and postCode != ''"> and post_code = #{postCode}</if>
        <if test="linkName != null  and linkName != ''"> and link_name like concat('%', #{linkName}, '%')</if>
